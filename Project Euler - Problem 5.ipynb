    "#simple approach, single number\n",
    "\n",
    "def ifDividesAll(num):\n",
    "    for i in range(2,21):\n",
    "        if num%i != 0:\n",
    "            return False\n",
    "    return True\n",
    "\n",
    "divisors = [n for n in range(2520,1000000000,20) if ifDividesAll(n) == True]\n",
    "\n",
    "print(divisors)"
   ]
